{
  "compilerOptions": {
    "jsx": "react-jsx",
    "noEmit": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["react/canary", "react-dom/canary"]
  },
  "include": ["src/"]
}
